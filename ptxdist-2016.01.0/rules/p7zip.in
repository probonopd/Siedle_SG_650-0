## SECTION=shell_and_console

config P7ZIP
	tristate
	select LIBC_M
	select LIBC_PTHREAD
	select GCCLIBS_GCC_S
	select GCCLIBS_CXX
	prompt "p7zip"
	help
	  p7zip is a port of 7za.exe for POSIX systems like Unix
	  (Linux, Solaris, OpenBSD, FreeBSD, Cygwin, AIX, ...),
	  MacOS X and also for BeOS and Amiga.
