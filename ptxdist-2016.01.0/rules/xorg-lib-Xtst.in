## SECTION=multimedia_xorg_lib

config XORG_LIB_XTST
	tristate
	prompt "libXtst"
	select XORG_PROTO_RECORD
	select XORG_PROTO_INPUT
	select XORG_PROTO_XEXT
	select XORG_LIB_X11
	select XORG_LIB_XEXT
	select XORG_LIB_XI
