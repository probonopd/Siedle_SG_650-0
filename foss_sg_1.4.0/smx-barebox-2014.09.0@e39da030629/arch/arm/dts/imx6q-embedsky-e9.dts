/dts-v1/;

#include "imx6q.dtsi"
#include <arm/imx6q-embedsky-e9.dtsi>

/ {
	chosen {
		linux,stdout-path = &uart4;

		environment@0 {
			compatible = "barebox,environment";
			device-path = &usdhc2, "partname:barebox-environment";
		};

	};
};

&usdhc2 {
	#address-cells = <1>;
	#size-cells = <1>;

	partition@0 {
		label = "barebox";
		reg = <0x0 0x80000>;
	};
	partition@1 {
		label = "barebox-environment";
		reg = <0x80000 0x80000>;
	};
};
