#include <arm/imx6qdl-sabresd.dtsi>

&ocotp {
	barebox,provide-mac-address = <&fec 0x620>;
};

&usbh1 {
	phy_type = "utmi";
	dr_mode = "host";
};

&usbotg {
	phy_type = "utmi";
};

&usdhc3 {
	#address-cells = <1>;
	#size-cells = <1>;

	partition@0 {
		label = "barebox";
		reg = <0x0 0x80000>;
	};

	partition@1 {
		label = "barebox-environment";
		reg = <0x80000 0x80000>;
	};
};
